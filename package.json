{
  "name": "blacktie",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && yarn styleguide:build && yarn content:build gulp reports",
    "lint": "vue-cli-service lint",
    "styleguide": "vue-styleguidist server --open",
    "styleguide:build": "vue-styleguidist build",
    "content": "eleventy --serve",
    "content:build": "eleventy",
    "analyze": "yarn webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.5.4",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-prettier": "^3.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "favicons-webpack-plugin": "^0.0.9",
    "gulp": "^3.9.1",
    "gulp-copy": "^4.0.0",
    "gulp-print": "^5.0.0",
    "gulp-sizereport": "^1.2.0",
    "gulp-string-replace": "^1.1.1",
    "lerna": "^3.4.0",
    "localforage": "^1.7.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "smooth-scroll": "^14.2.1",
    "umbrellajs": "^3.1.0",
    "validate": "^4.5.1",
    "vue-styleguidist": "^1.8.9",
    "vue-template-compiler": "^2.5.17"
  }
}
